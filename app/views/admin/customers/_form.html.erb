  <div class="field row form-inline m-2">
    <div class="col-3">氏名</div>
    <%= f.text_field :last_name, autofocus: true, placeholder: "長野", class: "form-control mr-3" %>
    <%= f.text_field :first_name, placeholder: "惠喜", class: "form-control" %>
  </div>
  <div class="field row form-inline m-2">
    <div class="col-3">フリガナ</div>
    <%= f.text_field :last_name_kana, placeholder: "ナガノ", class: "form-control mr-3" %>
    <%= f.text_field :first_name_kana, placeholder: "ケイキ", class: "form-control" %>
  </div>

  <div class="field row form-inline m-2">
    <%= f.label :postal_code, "郵便番号(ハイフンなし)", class: "col-3 justify-content-start" %>
    <%= f.text_field :postal_code, placeholder: "1234567", class: "form-control" %>
  </div>

  <div class="field row form-inline m-2">
    <%= f.label :address, "住所", class: "col-3 justify-content-start" %>
    <%= f.text_field :address, placeholder: "東京都千代田区千代田1-1", class: "form-control col-6" %>
  </div>

  <div class="field row form-inline m-2">
    <%= f.label :telephone_number, "電話番号(ハイフンなし)", class: "col-3  justify-content-start" %>
    <%= f.text_field :telephone_number, placeholder: "09012345678", class: "form-control" %>
  </div>

  <div class="field row form-inline m-2">
    <%= f.label :email, "メールアドレス", class: "col-3 justify-content-start" %>
    <%= f.email_field :email, placeholder: "mail@example.com", class: "form-control" %>
  </div>
