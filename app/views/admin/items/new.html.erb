<%= render "layouts/section", title: "商品新規登録" %>
<%= form_with model: @item, url: admin_items_path, method: :post, local: true do |f| %>
  <%= render "layouts/error_messages", resource: @item %>
  <%= render "form", f: f %>
  <%= f.submit "新規登録", class: "btn btn-success d-block mx-auto" %>
<% end %>